{
  "name": "traitify-widgets",
  "version": "0.0.0",
  "description": "Traitiy Widgets",
  "scripts": {
    "dev": "webpack --watch & serve .",
    "start": "serve build -s -c 1",
    "prestart": "npm run build",
    "build": "cross-env NODE_ENV=production webpack -p --progress",
    "prebuild": "mkdirp build",
    "test": "npm run -s lint && npm run -s test:karma",
    "test:karma": "karma start test/karma.conf.js --single-run",
    "test:karma:live": "karma start test/karma.chrome.conf.js",
    "test:karma:browserstack": "karma start test/karma.browserstack.conf.js",
    "lint": "eslint {src,test}",
    "serve": "serve ."
  },
  "keywords": [
    "traitify",
    "personality",
    "widgets"
  ],
  "license": "MIT",
  "author": "Traitify <support@traitify.com> (www.traitify.com)",
